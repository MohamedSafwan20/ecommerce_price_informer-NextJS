generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id           Int      @id @default(autoincrement())
  link         String
  imageUrl     String
  name         String
  store        String
  interval     Int
  orderedPrice Int
  createdAt    DateTime @default(now())
}
